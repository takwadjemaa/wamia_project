<odoo>
    <data noupdate="1">
         <record id="wk_product_category_coupon" model="product.category">
            <field name="name">Gift Voucher</field>
        </record>

        <record id="product_product_coupon" model="product.product">
            <field name="name">Gift Voucher</field>
            <field name="type">service</field>
            <field name="categ_id" ref="wk_coupons.wk_product_category_coupon"/>
            <field name="available_in_pos">True</field>
            <field name="image_1920" type="base64" file="wk_coupons/static/src/img/Gift-Voucher.png"/>
<!--            <field name="image_small" type="base64" file="wk_coupons/static/src/img/Gift-Voucher.png"/>-->
            <field name="taxes_id" eval="[(6,0,[])]"></field>
            <field name="supplier_taxes_id" eval="[(6,0,[])]"></field>
        </record>

</data>
</odoo>
